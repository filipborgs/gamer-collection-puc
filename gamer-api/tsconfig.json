{
  "compilerOptions": {
      "outDir": "dist",
      "module": "commonjs",
      "target": "es2019",
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "moduleResolution": "Node",
      "sourceMap": true,
      "rootDir": "src",
      "strictNullChecks": true,
      "baseUrl": "src",
      "paths": {
          "@/*": [
              "*"
          ],
      }
  },
  "include": [
      "src"
  ]
}