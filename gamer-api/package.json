{
    "name": "gamer-api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm run build && node dist/main/server.js",
        "test": "jest --passWithNoTests --silent --noStackTrace --runInBand --no-cache",
        "test:watch": "npm test -- --watch --coverage",
        "build": "rm -Rf ./dist && tsc -p tsconfig-build.json",
        "dev": "nodemon --inspect src/main/server.ts",
        "prepare": "cd .. && husky install",
        "pre-commit": "lint-staged",
        "typeorm": "typeorm-ts-node-esm -d ormconfig.ts",
        "migration:run": "npm run typeorm -- migration:run",
        "migration:revert": "npm run typeorm -- migration:revert"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.14.6",
        "@typescript-eslint/eslint-plugin": "^5.54.1",
        "eslint": "^8.35.0",
        "eslint-config-standard-with-typescript": "^34.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "git-commit-msg-linter": "^4.8.3",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-mock-extended": "^3.0.3",
        "lint-staged": "^13.1.2",
        "nodemon": "^2.0.21",
        "pg-mem": "^2.6.9",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "axios": "^1.3.4",
        "bcrypt": "^5.1.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "module-alias": "^2.2.2",
        "pg": "^8.10.0",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.3.12"
    },
    "_moduleAliases": {
        "@": "dist"
    }
